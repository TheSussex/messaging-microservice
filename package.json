{
  "name": "messaging-microservice",
  "main": "app.js",
  "scripts": {
    "lint": "eslint --fix src/ --ext .js",
    "dev": "nodemon --exec babel-node src/app.js",
    "send": "babel-node src/app/client.js",
    "receive": "babel-node src/app/transaction.js",
    "migrate:create": "db-migrate create --config database.json",
    "migrate:up": "db-migrate up --config database.json",
    "migrate:down": "db-migrate down --config database.json",
    "migrate:reset": "db-migrate reset --config database.json"
  },
  "dependencies": {
    "@babel/node": "^7.19.1",
    "amqplib": "^0.10.3",
    "bcrypt": "5.0.1",
    "bluebird": "^3.7.2",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.3",
    "express": "4.17.2",
    "helmet": "4.6.0",
    "http-status": "^1.5.0",
    "https": "^1.0.0",
    "joi": "^17.5.0",
    "jsonwebtoken": "8.5.1",
    "mongoose": "6.1.6",
    "nanoid": "^3.3.1",
    "pg-promise": "^11.4.3",
    "winston": "^3.5.1",
    "winston-daily-rotate-file": "^4.6.0",
    "winston-loggly-bulk": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/register": "^7.18.9",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.15"
  }
}
