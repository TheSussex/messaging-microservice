{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "max-len": ["error", { "code": 150, "tabWidth": 4 }],
        "indent": ["error", 2],
        "quotes": [
          "error",
          "single",
          {
            "avoidEscape": true
          }
        ],
        "no-unused-vars": "off",
        "import/prefer-default-export": "off"
    }
}
